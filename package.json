  "license": "MIT",
  "typings": "dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rm -rf ~/.degit && npm cache clean --force && yarn cache clean --force && yarn",
    "start": "tsdx watch",
    "prebuild": "yarn lint",
    "build": "tsdx build",
    "write": "yarn build && node scripts/write.mjs",
    "postwrite": "yarn build",
    "prepare": "yarn write",
    "test": "tsdx test",
    "posttest": "node test/import.mjs && node test/require.cjs",
    "lint": "tsdx lint src --fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "install-husky": "husky install"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/rainbow-token-list.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rainbow-token-list.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.0",
    "@types/degit": "^2.8.0",
    "@types/graceful-fs": "^4.1.5",
    "@types/lodash": "^4.14.170",
    "@types/lodash-es": "^4.17.4",
    "@types/logger": "^0.0.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.7",
    "size-limit": "^5.0.0",
    "tsdx": "ctjlewis/tsdx",
    "tslib": "^2.3.0",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@ethersproject/address": "^5.4.0",
    "@types/request": "^2.48.5",
    "@uniswap/token-lists": "rainbow-me/token-lists#40babff279b578e5585d274e1c7c93f35c56ed73",
    "commander": "^8.0.0",
    "decompress": "^4.2.1",
    "degit": "^2.8.0",
    "get-svg-colors": "^1.5.1",
    "graceful-fs": "^4.2.6",
    "lambda-git": "^0.1.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "logger": "^0.0.1",
    "make-color-more-chill": "ctjlewis/make-color-more-chill",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.1",
    "p-limit": "^3.1.0",
    "path": "^0.12.7",
    "rainbow-overrides": "ctjlewis/rainbow-overrides",
    "request": "^2.88.2",
    "simple-git": "^2.40.0",
    "zod": "^3.5.1"
  }
}
